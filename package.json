    "clean": "rm -rf lib",
    "build": "babel src -d lib --ignore src/spec",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdluo/babel-plugin-directory-named-module.git"
  },
  "author": "Mingdong Luo",
